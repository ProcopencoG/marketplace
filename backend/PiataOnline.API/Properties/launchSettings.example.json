{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "http://localhost:5066",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASE_URL": "Host=localhost;Port=5432;Database=piataonline;Username=postgres;Password=YOUR_PASSWORD",
        "JWT_SECRET": "YOUR_JWT_SECRET_MINIMUM_32_CHARACTERS_LONG",
        "JwtSettings__OwnerEmail": "your-admin-email@example.com"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "https://localhost:7236;http://localhost:5066",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASE_URL": "Host=localhost;Port=5432;Database=piataonline;Username=postgres;Password=YOUR_PASSWORD",
        "JWT_SECRET": "YOUR_JWT_SECRET_MINIMUM_32_CHARACTERS_LONG",
        "JwtSettings__OwnerEmail": "your-admin-email@example.com"
      }
    }
  }
}
